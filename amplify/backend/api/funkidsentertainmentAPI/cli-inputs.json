{
  "version": 1,
  "serviceConfiguration": {
    "apiName": "funkidsentertainmentAPI",
    "serviceName": "AppSync",
    "defaultAuthType": {
      "mode": "AMAZON_COGNITO_USER_POOLS",
      "cognitoUserPoolId": "authFunKidsEntertainment"
    },
    "conflictResolution": {
      "defaultResolutionStrategy": {
        "type": "AUTOMERGE"
      }
    },
    "additionalAuthTypes": [
      {
        "mode": "AWS_IAM"
      },
      {
        "mode": "API_KEY",
        "expirationTime": 30,
        "keyDescription": "api key description"
      }
    ]
  }
}